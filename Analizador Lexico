package analizadorlexico;

import java.io.File;

/**
 *
 * @author USER
 */
public class AnalizadorLexico {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        String lectura = "C:/Users/USER/Documents/NetBeansProjects/Analizador Lexico/src/analizadorlexico/lexico.flex";
        generarLexico(lectura);

    }
    public static void generarLexico(String lectura){
        File file=new File(lectura);
        jflex.Main.generate(file);
        new interfaslexica().setVisible(true);
        
    }
    }
    
